{
    "name": "yevhenii/telegram",
    "description": ":send notification to telegram bot",
    "license": "MIT",
    "authors": [
        {
            "name": "Yevhenii Riabyi",
            "email": "zenia9012@gmail.com"
        }
    ],
    "homepage": "https://github.com/yevhenii/telegram",
    "keywords": ["Laravel", "Telegram"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Yevhenii\\Telegram\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yevhenii\\Telegram\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yevhenii\\Telegram\\TelegramServiceProvider"
            ],
            "aliases": {
                "Telegram": "Yevhenii\\Telegram\\Facades\\Telegram"
            }
        }
    }
}
